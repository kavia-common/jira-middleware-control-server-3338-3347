# Authentication for clients (Bearer token)
AUTH_TOKEN=replace-with-strong-random-token

# JIRA credentials
JIRA_BASE_URL=https://your-domain.atlassian.net
JIRA_EMAIL=your-email@example.com
JIRA_API_TOKEN=your-api-token
JIRA_CLOUD_INSTANCE=true

# CORS configuration (comma-separated origins). Use * for all.
# Example: http://localhost:5173,https://your.app.com
CORS_ALLOW_ORIGINS=*

# Server port (preview environment uses 3001)
PORT=3001

# Debug logging
DEBUG=false
