# JIRA MCP Backend - .env.example
# Copy to .env and fill values as needed.

# JIRA configuration (required for JIRA endpoints)
JIRA_EMAIL=your-email@example.com
JIRA_API_TOKEN=your-api-token
# Either provide JIRA_BASE_URL or JIRA_CLOUD_SITE
# JIRA_BASE_URL=https://your-site.atlassian.net
JIRA_CLOUD_SITE=your-site-key

# Application security - comma-separated list of allowed API keys
# Requests must include X-API-KEY: <key> or Authorization: Bearer <key>
APP_API_KEYS=change-me-strong-key

# CORS - comma-separated origins (defaults to *)
# APP_CORS_ORIGINS=https://your-frontend.example.com,https://another.example.com

# Logging and environment
LOG_LEVEL=INFO
APP_ENV=development
